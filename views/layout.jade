doctype 5
html(xmlns='http://www.w3.org/1999/xhtml', xmlns:og='http://ogp.me/ns#', xmlns:fb='https://www.facebook.com/2008/fbml')


	head
		title= title
		meta(property='og:image', content='images/hackm.png')

		link(rel='stylesheet', href='/stylesheets/style.css')
		script(src="/javascripts/jquery.min.js")
	body
		.container
			.nav
				.nav-title
					.pres Michigan Hackers Present:
					a(href="/")=title
				.nav-sub 
					a.link(href="/prizes") prizes
					a.link(href="/schedule") schedule
					a.link(href="/sponsors") sponsors
					a.link(href="/contact") contact

			block content
			p.foot Build by 
				a(href="http://www.michiganhackers.org") Michigan Hackers
				|  in Ann Arbor
			p.left
				a(href="https://www.facebook.com/michiganhackers"): img(src="images/facebook.png")
				a(href="https://twitter.com/MichiganHackers"): img(src="images/twitter.png")